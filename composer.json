{
    "type": "project",
    "license": "proprietary",
    "name": "enhavo/appskeleton",
    "description": "",
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.*",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.14",
        "enhavo/api-bundle": "dev-master",
        "enhavo/app-bundle": "dev-master",
        "enhavo/block-bundle": "dev-master",
        "enhavo/content-bundle": "dev-master",
        "enhavo/dashboard-bundle": "dev-master",
        "enhavo/doctrine-extension-bundle": "dev-master",
        "enhavo/form-bundle": "dev-master",
        "enhavo/media-bundle": "dev-master",
        "enhavo/metadata": "dev-master",
        "enhavo/navigation-bundle": "dev-master",
        "enhavo/page-bundle": "dev-master",
        "enhavo/routing-bundle": "dev-master",
        "enhavo/setting-bundle": "dev-master",
        "enhavo/type": "dev-master",
        "enhavo/user-bundle": "dev-master",
        "enhavo/vue-form-bundle": "dev-master",
        "rollerworks/password-strength-bundle": "^2.2",
        "symfony/console": "^6.3",
        "symfony/dotenv": "^6.3",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^6.3",
        "symfony/http-client": "^6.3",
        "symfony/twig-bundle": "^6.3",
        "symfony/yaml": "^6.3"
    },
    "require-dev": {
        "symfony/debug-bundle": "^6.3",
        "symfony/maker-bundle": "^1.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/stopwatch": "^6.3",
        "symfony/var-dumper": "^6.3",
        "symfony/web-profiler-bundle": "^6.3",
        "deployer/deployer": "^7.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "ocramius/package-versions": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.4.*",
            "docker": true
        }
    }
}
